    <!ELEMENT name (#PCDATA)>
    <!ELEMENT class (#PCDATA)>
    <!ELEMENT email (#PCDATA)>
    <!ELEMENT number (#PCDATA)>
    <!ELEMENT password (#PCDATA)>

    
   

    <!ATTLIST aptech center CDATA #REQUIRED>
    <!ATTLIST faculty semester CDATA #IMPLIED>
    <!ATTLIST email domain CDATA #FIXED "gmail">
    
